<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RingProgressBar">
        <attr name="ringColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsShow" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>

        <attr name="textValue" format="string"></attr>
        <attr name="textTitle" format="string"></attr>
        <attr name="isShowValue" format="boolean"></attr>
        <attr name="isShowTitle" format="boolean"></attr>
        <attr name="textSizeValue" format="dimension"></attr>
        <attr name="textSizeTitle" format="dimension"></attr>
        <attr name="positionStart" format="integer"></attr>
        <attr name="colorTextValue" format="color"></attr>
        <attr name="isPrgCPU" format="boolean"></attr>
    </declare-styleable>
</resources>