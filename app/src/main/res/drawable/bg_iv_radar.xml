<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">


            <solid android:color="#10ffffff">

            </solid>

            <corners android:radius="@dimen/_5sdp"></corners>

            <padding
                android:bottom="@dimen/_20sdp"
                android:left="@dimen/_20sdp"
                android:right="@dimen/_20sdp"
                android:top="@dimen/_20sdp"></padding>
        </shape>

    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <solid android:color="#20ffffff"></solid>
            <stroke
                android:width="5px"
                android:color="#3fFAFAFA"></stroke>
            <corners android:radius="@dimen/_5sdp"></corners>


        </shape>
    </item>


</layer-list>